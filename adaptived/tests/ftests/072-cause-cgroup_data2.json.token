{
	"rules": [
		{
			"name": "Gather cgroup data and print it out",
			"causes": [
				{
					"name": "cgroup_data",
					"args": {
						"cgroup": "<< pwd >>/test072cgroup",
						"rel_paths": false,
						"max_depth": 10,
						"settings": [
							{
								"setting": "memory.current"
							},
							{
								"setting": "cpu.uclamp.min"
							},
							{
								"setting": "cpuset.cpus.effective"
							}
						]
					}
				}
			],
			"effects": [
				{
					"name": "print",
					"args": {
						"file": "072-cause-cgroup_data2.out",
						"shared_data": true
					}
				}
			]
		}
	]
}
